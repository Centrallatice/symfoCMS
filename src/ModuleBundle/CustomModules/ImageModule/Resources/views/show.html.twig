{% extends '::admin.html.twig' %}

{% block body %}
    <h1>Imagemodule</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ imageModule.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ imageModule.url }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('custom_module_image_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('custom_module_image_edit', { 'id': imageModule.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
